{
  "env": {
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "import/prefer-default-export": "off",
    "prefer-promise-reject-errors": "off",
    "no-throw-literal": "off",
    "quotes": ["error", "single", { "avoidEscape": true }]
  }
}
