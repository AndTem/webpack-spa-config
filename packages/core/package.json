{
  "name": "@webpackon/core",
  "version": "1.0.0",
  "description": "Webpack 4 config for SPA",
  "author": "And_tem",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndTem/webpack-spa-config"
  },
  "browser": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "homepage": "https://github.com/AndTem/webpack-spa-config#Readme",
  "keywords": [
    "webpack",
